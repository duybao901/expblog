<div class="mt-4">
    <div class="container">
        <h2 class="mb-4">Edit Video üçÄ</h2>
        <form method="POST" action="/course/{{course._id}}?_method=PUT">
            <div class="form-group">
                <label for="name">Name Course</label>
                <input type="text" class="form-control" id="name" name="name" value="{{course.name}}">
                <label for="description">Description</label>
                <input type="text" class="form-control" id="description" name="description" value="{{course.description}}">
                <label for="videoId">Video ID</label>
                <input type="text" class="form-control" id="videoId" name="videoId" value="{{course.videoId}}">
                <label for="levles">Level</label>
                <input type="text" class="form-control" id="levles" name="level" value="{{course.level}}">
            </div>
            <button type="submit" class="btn btn-primary">Save</button>
        </form>
    </div>
</div>