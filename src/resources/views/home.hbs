<div class="mt-4">
    <div class="container">
        <div class="row">
            {{#each courses}}
            <div class="card-course-itemcol-sm-12 col-md-6 col-lg-4">
                <div class="card">
                    <img src="{{this.image}}" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">{{this.name}}</h5>
                        <p class="card-text">{{this.description}}</p>
                        <a href="#" class="btn btn-primary">Learn</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>
</div>